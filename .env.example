# TrustVault PWA Environment Variables
# Copy this file to .env for local development

# Application Configuration
VITE_APP_NAME=TrustVault
VITE_APP_VERSION=1.0.0

# Security Configuration
VITE_SESSION_TIMEOUT_MINUTES=15
VITE_CLIPBOARD_CLEAR_SECONDS=30

# WebAuthn Configuration (for production)
VITE_WEBAUTHN_RP_NAME=TrustVault
VITE_WEBAUTHN_RP_ID=localhost
# VITE_WEBAUTHN_RP_ID=yourdomain.com (for production)

# API Configuration (if using backend)
# VITE_API_URL=https://api.trustvault.com

# Feature Flags
VITE_ENABLE_BIOMETRIC=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_IMPORT=true

# Development Only
VITE_DEV_TOOLS=true
